            <Summary xml:lang="en">Simple and Fast Multimedia Library</Summary>
            <Summary xml:lang="tr">Simple and Fast Multimedia Library (Basit ve Hızlı Çokluortam Kütüphanesi)</Summary>
            <Description xml:lang="en">SFML is a simple, fast, cross-platform, and object-oriented multimedia API.</Description>
            <Description xml:lang="tr">SFML; basit, hızlı, çok platformlu ve nesne yönelimli bir çokluortam uygulama programlama arayüzüdür.</Description>
            <Icon>sfml</Icon>
            <Archive type="targz" sha1sum="d228d19c72d891485479db8b70c783ae20c6350f">http://dl.dropbox.com/u/1836567/Paketler/LaurentGomila-SFML-17971ca.tar.gz</Archive>
            <BuildDependencies>
                <Dependency>freetype-devel</Dependency>
                <Dependency>glew-devel</Dependency>
                <Dependency>jpeg-devel</Dependency>
                <Dependency>libXrandr-devel</Dependency>
                <Dependency>libXrender-devel</Dependency>
                <Dependency>libpng-devel</Dependency>
                <Dependency>libsndfile-devel</Dependency>
                <Dependency>mesa-devel</Dependency>
                <Dependency>openal-devel</Dependency>
            </BuildDependencies>
            <SourceURI>sfml/pspec.xml</SourceURI>
        </Source>
        <Package>
            <Name>sfml</Name>
            <RuntimeDependencies>
                <Dependency>freetype</Dependency>
                <Dependency>glew</Dependency>
                <Dependency>jpeg</Dependency>
                <Dependency>libXrandr</Dependency>
                <Dependency>libXrender</Dependency>
                <Dependency>libsndfile</Dependency>
                <Dependency>openal</Dependency>
            </RuntimeDependencies>
            <Files>
                <Path fileType="library">/usr/lib</Path>
                <Path fileType="data">/usr/share/pixmaps</Path>
            </Files>
            <AdditionalFiles>
                <AdditionalFile target="/usr/share/pixmaps/sfml.png" permission="0644" owner="root">sfml.png</AdditionalFile>
            </AdditionalFiles>
        </Package>
        <Package>
            <Name>sfml-devel</Name>
            <Summary xml:lang="en">Development files for SFML</Summary>
            <Summary xml:lang="tr">SFML için kütüphaneler</Summary>
            <Description xml:lang="en">SFML devel package provides development files for SFML.</Description>
            <Description xml:lang="tr">SFML geliştirme paketi, SFML için geliştirme dosyaları sunar.</Description>
            <IsA>data</IsA>
            <RuntimeDependencies>
                <Dependency release="1">sfml</Dependency>
            </RuntimeDependencies>
            <Files>
                <Path fileType="header">/usr/include</Path>
                <Path fileType="data">/usr/share/cmake/Modules</Path>
            </Files>
        </Package>
        <Package>
            <Name>sfml-docs</Name>
            <Summary xml:lang="en">Documentation files for SFML</Summary>
            <Summary xml:lang="tr">SFML için geliştirme belgeleri</Summary>
            <Description xml:lang="en">SFML docs package provides documentation files for SFML.</Description>
            <Description xml:lang="tr">SFML belge paketi, SFML için geliştirme belgeleri sunar.</Description>
            <IsA>data</IsA>
            <RuntimeDependencies>
                <Dependency release="1">sfml</Dependency>
            </RuntimeDependencies>
            <Files>
                <Path fileType="data">/usr/share/SFML</Path>
                <Path fileType="doc">/usr/share/doc</Path>
            </Files>
        </Package>
